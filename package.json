		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.35.1",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.1.10",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.6",
		"vitest": "^3.2.3"
	},
	"dependencies": {
		"@supabase/ssr": "^0.8.0",
		"@supabase/supabase-js": "^2.50.3",
		"fuse.js": "^7.1.0",
		"lucide-svelte": "^0.525.0"
	},
	"keywords": [
		"foxther",
		"sveltekit",
		"sandbox",
